package com.xinwei.kanfangshenqi.model;

import java.io.Serializable;
/**
 ********************
 * 回复信息
 * @author cn
 ********************
 */
public class ReplyComment implements Serializable {
	private String replycommentId;
	private String replyContent;
	private String replyMemberName;
	private String replyMemberHeadPortrait;
	private String replyParentMemberName;//"小丸子"
	public String getReplycommentId() {
		return replycommentId;
	}
	public void setReplycommentId(String replycommentId) {
		this.replycommentId = replycommentId;
	}
	public String getReplyContent() {
		return replyContent;
	}
	public void setReplyContent(String replyContent) {
		this.replyContent = replyContent;
	}
	public String getReplyMemberName() {
		return replyMemberName;
	}
	public void setReplyMemberName(String replyMemberName) {
		this.replyMemberName = replyMemberName;
	}
	public String getReplyMemberHeadPortrait() {
		return replyMemberHeadPortrait;
	}
	public void setReplyMemberHeadPortrait(String replyMemberHeadPortrait) {
		this.replyMemberHeadPortrait = replyMemberHeadPortrait;
	}
	public String getReplyParentMemberName() {
		return replyParentMemberName;
	}
	public void setReplyParentMemberName(String replyParentMemberName) {
		this.replyParentMemberName = replyParentMemberName;
	}
	@Override
	public String toString() {
		return "ReplayComment [replycommentId=" + replycommentId
				+ ", replyContent=" + replyContent + ", replyMemberName="
				+ replyMemberName + ", replyMemberHeadPortrait="
				+ replyMemberHeadPortrait + ", replyParentMemberName="
				+ replyParentMemberName + "]";
	}
	
}
