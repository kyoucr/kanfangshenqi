<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fafafafa"
    android:orientation="vertical"
     >
        <TextView
            android:id="@+id/company_name" 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="公司"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:layout_margin="@dimen/margin_normal"
            />
		<View
           android:layout_width="match_parent"
           android:layout_height="@dimen/login_input_line_height"
           android:background="@color/line" />
        <RelativeLayout
            android:id="@+id/rl_company_name"
            android:layout_width="match_parent"
            android:background="@drawable/bg_white"
            android:layout_height="@dimen/title_height"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/margin_normal"
            android:paddingRight="@dimen/margin_normal"
             >
             <com.xinwei.kanfangshenqi.view.ClearEditText
	       		android:id="@+id/company_address"
	        	android:layout_width="match_parent"
	        	android:background="@drawable/bg_white" 
	        	android:hint="请输入您的公司地址"
	        	android:saveEnabled="true"
	        	android:maxLength="40"
	        	android:singleLine="true"
	        	android:paddingRight="@dimen/margin_small"
	        	android:drawableRight="@drawable/delete_selector"
	        	android:layout_height="match_parent"
	        	android:textCursorDrawable="@null"
	        	android:layout_toLeftOf="@+id/tv_company_positioning"
	        	android:textColorHint="@color/text_gray_input"
	        	android:textSize="@dimen/txt_size_normal">
    		</com.xinwei.kanfangshenqi.view.ClearEditText>

             <TextView
                 android:id="@+id/tv_company_positioning"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="位"
                 android:textSize="@dimen/txt_little_normal"
                 android:textColor="@color/orange"
                 android:layout_toLeftOf="@+id/iv_company_positioning"
                 android:layout_centerVertical="true"
                 android:visibility="invisible"
                 />
    		 <ImageView 
    		     android:id="@+id/iv_company_positioning"
                 android:layout_width="26dp"
                 android:layout_height="26dp"
                 android:text="定位"
                 android:textSize="@dimen/txt_size_normal"
                 android:textColor="@color/orange"
    		     android:src="@drawable/location_button"
    		     android:layout_alignParentRight="true"
    		     android:layout_centerVertical="true"
    		     />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/login_input_line_height"
            android:background="@color/line" />
        <TextView
            android:id="@+id/home_name" 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="住址"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:layout_margin="@dimen/margin_normal"
            />
        <View
           android:layout_width="match_parent"
           android:layout_height="@dimen/login_input_line_height"
           android:background="@color/line" />
        <RelativeLayout
            android:id="@+id/verification_code"
            android:layout_width="match_parent"
            android:layout_height="@dimen/title_height"
            android:paddingLeft="@dimen/margin_normal"
            android:paddingRight="@dimen/margin_normal"
            android:background="@drawable/bg_white"
             >
    		 
            <com.xinwei.kanfangshenqi.view.ClearEditText
                android:id="@+id/home_address"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:background="@color/white"
                android:hint="请输入您的住址"
                android:saveEnabled="true"
                android:layout_weight="15"
                android:singleLine="true"
	        	android:maxLength="40"
	        	android:paddingRight="@dimen/margin_small"
                android:textCursorDrawable="@null"
                android:textColorHint="@color/text_gray_input"
                android:layout_toLeftOf="@+id/tv_home_positioning"
                android:textSize="16sp" />
            <TextView
                 android:id="@+id/tv_home_positioning"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_weight="1"
                 android:text="位"
                 android:textSize="@dimen/txt_little_normal"
                 android:textColor="@color/orange"
                 android:layout_toLeftOf="@+id/iv_home_positioning"
                 android:layout_centerVertical="true"
                 android:visibility="invisible"
                 />
    		 <ImageView 
    		     android:id="@+id/iv_home_positioning"
                 android:layout_width="26dp"
                 android:layout_height="26dp"
                 android:text="定位"
                 android:textSize="@dimen/txt_size_normal"
                 android:textColor="@color/orange"
    		     android:src="@drawable/location_button"
    		     android:layout_gravity="center"
    		     android:layout_alignParentRight="true"
    		     android:layout_centerVertical="true"
    		     />
        </RelativeLayout>
        <View
           android:layout_width="match_parent"
           android:layout_height="@dimen/login_input_line_height"
           android:background="@color/line" />
</LinearLayout>
	
